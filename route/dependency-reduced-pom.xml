<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com</groupId>
  <artifactId>route</artifactId>
  <name>route</name>
  <version>0.0.1-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>utf-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.0.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <transformers>
                <transformer>
                  <manifestEntries>
                    <Premain-Class>com.javaagent.root</Premain-Class>
                  </manifestEntries>
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>3dmsv2</id>
      <name>VRKB Repository</name>
      <url>http://maven.aliyun.com/nexus/content/groups/public</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <id>3dmsv2</id>
      <name>VRKB Repository</name>
      <url>http://maven.aliyun.com/nexus/content/groups/public</url>
    </pluginRepository>
  </pluginRepositories>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <properties>
    <spring.version>4.1.2.RELEASE</spring.version>
    <lombok.version>1.16.10</lombok.version>
    <commons-beanutils.version>1.9.3</commons-beanutils.version>
    <commons-io.version>2.5</commons-io.version>
    <mybatis.version>3.2.7</mybatis.version>
    <ezmorph.version>1.0.6</ezmorph.version>
    <downloadSources>true</downloadSources>
    <fastjson.version>1.2.37</fastjson.version>
    <druid.version>1.0.29</druid.version>
    <mysql.driver.version>5.1.42</mysql.driver.version>
    <slf4j.version>1.7.7</slf4j.version>
    <validator.version>5.2.4.Final</validator.version>
    <commons-lang3.version>3.5</commons-lang3.version>
    <commons-fileupload.version>1.3.3</commons-fileupload.version>
    <guava.version>19.0</guava.version>
    <jackson.version>2.5.4</jackson.version>
    <jsonobject.version>2.4</jsonobject.version>
    <commons-codec.version>1.9</commons-codec.version>
    <useragent.version>1.20</useragent.version>
    <mybatis-spring.version>1.2.3</mybatis-spring.version>
  </properties>
</project>

